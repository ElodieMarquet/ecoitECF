                    <a style="text-decoration: none; outline: none; color: #347273;" href={{ path('app_create_section_new') }}>Cr√©er une nouvelle section</a>
                </button>
            </div>
            <div class="formcreat">{{ form_row(form.name) }}</div>
            <div class="formcreat">{{ form_row(form.image) }}</div>
            <div class="formcreatmd">{{ form_row(form.description) }}</div>
            <div class="formcreatxl">{{ form_row(form.contenu) }}</div>
